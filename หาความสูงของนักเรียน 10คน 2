#include <stdio.h>
main(){
	int i,length;
	
	printf("\nInput number of students = ");
	scanf("%d",&length);
	
	int score[length];
	int sum = 0;
	float average =0;
	
	for(i=0;i<length;i++){
		printf("\nstudents %d = ",i+1);
		scanf("%d",&score[i]);
		sum += score[i];
	}
	average = sum/length;
	printf("\nAverage height of 10 students = %0.2f",average);
	return 0;
}
